{
  "compilerOptions": {
    "outDir": "./dist/",
    "noFallthroughCasesInSwitch": true,
    "declaration": false,
    "target": "es5",
    "module": "commonjs",
    "lib": [
      "es6",
      "es2017",
      "dom"
    ],
    "baseUrl": "src",
    "paths": {
      "~/*": [
        "*"
      ],
      "@rs-clair/expectance": [
        "index.ts"
      ]
    },
    // "sourceMap": true,
    "strict": true,
    "alwaysStrict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true
  },
  "include": [
    "./src/**/*",
    "./examples/**/*"
  ]
  // "esModuleInterop": true
}
